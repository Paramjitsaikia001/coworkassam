{
    "routes": [
        { "handle": "filesystem" },
        {
            "src": "/(.*)\\.js",
            "headers": { "content-type": "application/javascript" }
        },
        {
            "src": "/(.*)\\.mjs",
            "headers": { "content-type": "application/javascript" }
        },
        {
            "src": "/(.*)\\.ts",
            "headers": { "content-type": "application/javascript" }
        },
        {
            "src": "/(.*)\\.tsx",
            "headers": { "content-type": "application/javascript" }
        },
        { "src": "/(.*)", "dest": "/index.html" }
    ]
}
